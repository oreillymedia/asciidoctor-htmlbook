<%#encoding:UTF-8%><% terms = (attr :terms) %><% if terms %><% numterms = terms.size %><%
terms.each do |term| %><%
  if term.start_with?('see=') %><% $seeterm = term.gsub('see=', '') %><% end %><%
  if term.start_with?('seealso=') %><% $seealsoterm = term.gsub('seealso=', '') %><% end %><%
  if term.start_with?('sortas=') %><% $sortasterm = term.gsub('sortas=', '') %><% end %><%
  if term.start_with?('id=') %><% $idterm = term.gsub('id=', '') %><% end %><%
  if term.start_with?('range=') %><% $rangeterm = term.gsub('range=', '') %><% end %><%
  if term.start_with?('startref=') %><% $startrefterm = term.gsub('startref=', '') %><% end %><%
  end %><%
  unless terms[0].to_s.start_with?('range=', 'startref=') %><% primaryterm = terms[0] %><% end %><%
  unless terms[1].to_s.start_with?('see=', 'seealso=', 'sortas=', 'id=', 'range=', 'startref=') %><% secondaryterm = terms[1] %><% end %><%
  unless terms[2].to_s.start_with?('see=', 'seealso=', 'sortas=', 'id=', 'range=', 'startref=') %><% tertiaryterm = terms[2] %><%
  end %><%
  if numterms == 1
    %><a data-type="indexterm" data-primary="<%= primaryterm %>"/><%
  elsif numterms == 2 %><%
    if secondaryterm %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>"/><%
    elsif terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-see="<%= $seeterm %>"/><%
    elsif terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-seealso="<%= $seealsoterm %>"/><%
    elsif terms.to_s.include?('sortas=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-primary-sortas="<%= $sortasterm %>"/><%
    elsif terms.to_s.include?('id=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" id="<%= $idterm %>"/><%
    elsif terms.to_s.include?('startref=') %><a data-type="indexterm" data-startref="<%= $startrefterm %>"/><% end %><%
  elsif numterms == 3 %><%
    if tertiaryterm %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>"/><%
    elsif terms.to_s.include?('see=') and not terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-see="<%= $seeterm %>"/><%
    elsif terms.to_s.include?('seealso=') and not terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-seealso="<%= $seealsoterm %>"/><%
    elsif terms.to_s.include?('seealso=') and terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
    elsif terms.to_s.include?('sortas=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-secondary-sortas="<%= $sortasterm %>"/><%
    elsif terms.to_s.include?('range=startofrange') %><a data-type="indexterm" data-primary="<%= primaryterm %>" id="<%= $idterm %>"/><%
    elsif terms.to_s.include?('startref=') %><a data-type="indexterm" data-startref="<%= $idterm %>"/><% end %><%
  elsif numterms == 4 %><%
    if not secondaryterm and not tertiaryterm %><%
      if terms.to_s.include?('see=') and terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') and terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') and terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" id="<%= $idterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') and terms.to_s.include?('sortas=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-primary-sortas="<%= $sortasterm %>" id="<%= $idterm %>"/><%
      elsif terms.to_s.include?('startref=') %><a data-type="indexterm" data-startref="<%= $idterm %>"/><% end %><%
    elsif secondaryterm and not tertiaryterm %><%
      if terms.to_s.include?('see=') and terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" id="<%= $idterm %>"/><% end %><%
    elsif tertiaryterm %><%
      if terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" data-see="<%= $seeterm %>"/><%
      elsif terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('sortas=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" data-tertiary-sortas="<%= $sortasterm %>"/><% end %><% end %><%
  elsif numterms == 5 %><%
    if not secondaryterm and not tertiaryterm
      %><a data-type="indexterm" id="<%= $idterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
    elsif secondaryterm and not tertiaryterm %><%
      if terms.to_s.include?('range=startofrange') and terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') and terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" id="<%= $idterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') and terms.to_s.include?('sortas=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-secondary-sortas="<%= $sortasterm %>" id="<%= $idterm %>"/><%
      elsif terms.to_s.include?('see=') and terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><% end %><%
    elsif tertiaryterm %><%
      if terms.to_s.include?('see=') and terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('range=startofrange') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" id="<%= $idterm %>"/><% end %><% end %><%
  elsif numterms == 6 %><%
    if secondaryterm and not tertiaryterm
      %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
    elsif tertiaryterm %><%
      if terms.to_s.include?('see=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>"/><%
      elsif terms.to_s.include?('seealso=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" id="<%= $idterm %>" data-seealso="<%= $seealsoterm %>"/><%
      elsif terms.to_s.include?('sortas=') %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" data-tertiary-sortas="<%= $sortasterm %>" id="<%= $idterm %>"/><% end %><% end %><%
  elsif numterms == 7
    %><a data-type="indexterm" data-primary="<%= primaryterm %>" data-secondary="<%= secondaryterm %>" data-tertiary="<%= tertiaryterm %>" id="<%= $idterm %>" data-see="<%= $seeterm %>" data-seealso="<%= $seealsoterm %>"/><%
end %><% end %>
